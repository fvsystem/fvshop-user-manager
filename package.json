{
  "name": "@fvsystem/fvshop-user-manager",
  "version": "0.2.10",
  "description": "User Manager system for FVShop",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "export": {
    ".": {
      "require": "./dist/cjs/index.js",
      "types": "./dist/cjs/index.d.ts",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "pretest": "tsc --noEmit",
    "test": "jest",
    "prepublish": "npm run build",
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && tscpaths -p tsconfig.json -s ./src -o ./dist/cjs",
    "build:esm": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist/esm",
    "commit": "cz",
    "start-express-sequelize": "node user/infrastructure/app/express-sequelize",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/user/infrastructure/app/express-sequelize' -e ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fvsystem/fvshop-user-manager.git"
  },
  "author": "Lucas Rego",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fvsystem/fvshop-user-manager/issues"
  },
  "homepage": "https://github.com/fvsystem/fvshop-user-manager#readme",
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.22",
    "@swc/jest": "^0.2.24",
    "@types/express": "^4.17.15",
    "@types/jest": "latest",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "commitizen": "latest",
    "cz-conventional-changelog": "latest",
    "eslint": "latest",
    "eslint-config-airbnb-base": "latest",
    "eslint-config-prettier": "latest",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-prettier": "latest",
    "husky": "latest",
    "jest": "latest",
    "nodemon": "latest",
    "prettier": "latest",
    "ts-jest": "latest",
    "ts-node": "latest",
    "tsconfig-paths": "^4.1.1",
    "tscpaths": "^0.0.9",
    "typescript": "latest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@fvsystem/fvshop-identity": "^0.2.0",
    "@fvsystem/fvshop-shared-entities": "^0.1.33",
    "axios": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "sqlite3": "^5.1.4",
    "supertest": "^6.3.3",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  }
}
